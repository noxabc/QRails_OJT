<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
  <thead>
    <tr>
      <th>ID</th>
      <th>Last Name</th>
      <th>First Name</th>
      <th>Middle Name</th>
      <th>Username</th>
      <th>Mobile Number</th>
      <th>Date Created</th>
    </tr>
  </thead>

  <tbody>
    <% @user.each do |user| %>
    <tr>
      <td><%= user.id%></td>
      <td><%= user.last_name%></td>
      <td><%= user.first_name%></td>
      <td><%= user.middle_name%></td>
      <td><%= user.username%></td>
      <td><%= user.mobile_number%></td>
      <td><%= user.created_at%></td>
    </tr>
    <% end %>
  </tbody>
</table>
