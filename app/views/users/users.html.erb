<div class="users_t">
    <table>
          
      <tr>
        <th>User ID</th>
        <th>Last Name</th>
        <th>First Name</th>
        <th>Middle Name</th>
        <th>Email</th>
        <th>Mobile Number</th>
      </tr>

      <% @users.each do |user| %>
      <tr>
          <td> <%= user.id %> </td>
          <td> <%= user.last_name %> </td>
          <td> <%= user.first_name %> </td>
          <td> <%= user.middle_name %> </td>
          <td> <%= user.email %> </td>
          <td> <%= user.mobile_number %> </td>
      </tr>
      <% end %>

    </table>
</div>
